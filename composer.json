{
    "name": "monkblog/monkblog-php",
    "description": "MonkBlog a blogging engine.",
    "keywords": ["monk", "blog", "monkblog", "laravel", "blogging"],
    "license": "MIT",
    "type": "project",
    "authors": [
      {
        "name": "Eric Dowell",
        "email": "eric@ericodwell.com"
      },
      {
        "name": "Ben Overmyer",
        "email": "manatrance@gmail.com"
      }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "monkblog/theme-manager": "1.1.*",
        "barryvdh/laravel-debugbar": "^2.0",
        "barryvdh/laravel-ide-helper": "^2.0",
        "laravelcollective/html": "~5.0",
        "michelf/php-markdown": "1.4.1",
        "rcrowe/twigbridge": "~0.7",
        "twig/extensions": "~1.2.0",
        "pda/pheanstalk": "^3.0",
        "filp/whoops": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "codeclimate/php-test-reporter": "dev-master",
        "laravel/homestead": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "MonkBlog\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.2-dev"
      }
    },
    "config": {
        "preferred-install": "dist"
    }
}
